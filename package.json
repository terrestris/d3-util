{
  "name": "@terrestris/d3-util",
  "version": "0.9.2",
  "description": "A set of helper classes for working with d3 charts.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "release": "np --no-yarn && git push https://github.com/terrestris/d3-util.git master --tags",
    "lint": "eslint --ext js src/",
    "test": "jest --maxWorkers=4 --coverage",
    "test:watch": "jest --watchAll",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "clean:dist": "rimraf ./dist/*",
    "build:dist": "npm run clean:dist && webpack --config browser-build.config.js",
    "build:dev": "npm run clean:dist && webpack --mode=development",
    "start:dev": "webpack --config dev-build.config.js --watch --mode=development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terrestris/d3-util.git"
  },
  "author": "terrestris GmbH & Co. KG <info@terrestris.de>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/terrestris/d3-util/issues"
  },
  "homepage": "https://github.com/terrestris/d3-util#readme",
  "dependencies": {},
  "peerDependencies": {
    "d3": "^5",
    "d3-tip": "^0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-function-bind": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@types/d3-array": "^2",
    "@types/d3-axis": "^1",
    "@types/d3-color": "^1",
    "@types/d3-format": "^1",
    "@types/d3-scale": "^2.2.4",
    "@types/d3-selection": "^1",
    "@types/d3-shape": "^1.3.5",
    "@types/d3-time": "^1",
    "@types/d3-time-format": "^2",
    "@types/d3-tip": "^3",
    "@types/d3-transition": "^1.3.1",
    "@types/d3-zoom": "^1",
    "@types/jest": "^26.0.20",
    "awesome-typescript-loader": "^5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26",
    "babel-loader": "^8.2.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-import": "^1.13.3",
    "coveralls": "^3.1.0",
    "d3": "^5.16.0",
    "d3-tip": "^0",
    "d3-xyzoom": "^1.5.0",
    "eslint": "^7.19.0",
    "jest": "^26",
    "moment": "^2.29.1",
    "np": "^7.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.1",
    "typedoc": "^0.20.23",
    "typescript": "^4.1.4",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "whatwg-fetch": "^3"
  }
