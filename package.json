{
  "name": "@terrestris/d3-util",
  "version": "0.4.0",
  "description": "A set of helper classes for working with d3 charts.",
  "main": "src/index.js",
  "scripts": {
    "release": "np --no-yarn && git push https://github.com/terrestris/d3-util.git master --tags",
    "lint": "eslint --ext js src/",
    "test": "jest --maxWorkers=4",
    "test:watch": "jest --watchAll",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "clean:dist": "rimraf ./dist/*",
    "build:dist": "npm run clean:dist && webpack",
    "build:dev": "npm run clean:dist && webpack --mode=development",
    "start:dev": "npm run clean:dist && webpack --mode=development --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terrestris/d3-util.git"
  },
  "author": "terrestris GmbH & Co. KG <info@terrestris.de>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/terrestris/d3-util/issues"
  },
  "homepage": "https://github.com/terrestris/d3-util#readme",
  "dependencies": {},
  "peerDependencies": {
    "d3": "~5",
    "d3-tip": "0.9.1"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-function-bind": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "babel-plugin-import": "1.11.0",
    "babel-polyfill": "6.26.0",
    "coveralls": "3.0.2",
    "d3": "5.7.0",
    "d3-tip": "0.9.1",
    "eslint": "5.9.0",
    "jest": "23.6.0",
    "moment": "2.22.2",
    "np": "3.1.0",
    "rimraf": "2.6.3",
    "webpack": "4.28.3",
    "webpack-cli": "3.2.0",
    "whatwg-fetch": "3.0.0"
  }
}
